  gem "minitest"
  gem "minitest-hooks"
  gem "minitest-reporters"
  gem "minitest-focus"
  gem "minitest-stub-const"
  gem "mocha"
  gem "rake"
  gem "rubocop", require: false
  gem "rubocop-rake"
  gem "rubocop-minitest"
  gem "webrick"
end
